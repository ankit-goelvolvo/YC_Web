@{
    ViewBag.Title = "Home Page";
}

<div>


    <div class="hero-wrap js-fullheight" style="background-image: url('../Images/bg_1.jpg');">
        <div class="overlay"></div>
        <div class="container">
            <div class="row no-gutters slider-text js-fullheight align-items-center justify-content-start" data-scrollax-parent="true">
                <div class="col-md-9 ftco-animate" data-scrollax=" properties: { translateY: '70%' }">
                    <h1 class="mb-4" data-scrollax="properties: { translateY: '30%', opacity: 1.6 }"><strong>Explore <br></strong> your amazing city</h1>
                    <p data-scrollax="properties: { translateY: '30%', opacity: 1.6 }">Find great places to stay, eat, shop, or visit from local experts</p>
                </div>
            </div>
        </div>
    </div>
    <section class="ftco-section services-section bg-light">
        <div class="container">
            <div class="row d-flex">
                <div class="col-md-3 d-flex align-self-stretch ftco-animate" onclick="OpenLocation();">
                    <div class="media block-6 services d-block text-center">
                        <div class="d-flex justify-content-center"><div class="icon"><span class="icon-map-pin"></span></div></div>
                        <div class="media-body p-2 mt-2">
                            <h3 class="heading mb-3">Location</h3>
                        </div>
                    </div>
                </div>
                <div class="col-md-3 d-flex align-self-stretch ftco-animate" onclick="OpenOfficeType();">
                    <div class="media block-6 services d-block text-center">
                        <div class="d-flex justify-content-center"><div class="icon"><span class="icon-address-card"></span></div></div>
                        <div class="media-body p-2 mt-2">
                            <h3 class="heading mb-3">Office Types</h3>
                        </div>
                    </div>
                </div>
                <div class="col-md-3 d-flex align-self-stretch ftco-animate" onclick="OpenOffices();">
                    <div class="media block-6 services d-block text-center">
                        <div class="d-flex justify-content-center"><div class="icon"><span class="icon-account_box"></span></div></div>
                        <div class="media-body p-2 mt-2">
                            <h3 class="heading mb-3">Offices</h3>
                        </div>
                    </div>
                </div>
                <div class="col-md-3 d-flex align-self-stretch ftco-animate" onclick="OpenOfficeFacility();">
                    <div class="media block-6 services d-block text-center">
                        <div class="d-flex justify-content-center"><div class="icon"><span class="icon-cutlery"></span></div></div>
                        <div class="media-body p-2 mt-2">
                            <h3 class="heading mb-3">Office Facilities</h3>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row d-flex">
                <div class="col-md-3 d-flex align-self-stretch ftco-animate" onclick="OpenPolicies();">
                    <div class="media block-6 services d-block text-center">
                        <div class="d-flex justify-content-center"><div class="icon"><span class="icon-assignment"></span></div></div>
                        <div class="media-body p-2 mt-2">
                            <h3 class="heading mb-3">Policies</h3>
                        </div>
                    </div>
                </div>
                <div class="col-md-3 d-flex align-self-stretch ftco-animate" onclick="OpenRoomType();">
                    <div class="media block-6 services d-block text-center">
                        <div class="d-flex justify-content-center"><div class="icon"><span class="icon-home"></span></div></div>
                        <div class="media-body p-2 mt-2">
                            <h3 class="heading mb-3">Room Types</h3>
                        </div>
                    </div>
                </div>
                <div class="col-md-3 d-flex align-self-stretch ftco-animate" onclick="OpenRoomTypeOption();">
                    <div class="media block-6 services d-block text-center">
                        <div class="d-flex justify-content-center"><div class="icon"><span class="icon-shopping-basket"></span></div></div>
                        <div class="media-body p-2 mt-2">
                            <h3 class="heading mb-3">Room Type Options</h3>
                        </div>
                    </div>
                </div>
                <div class="col-md-3 d-flex align-self-stretch ftco-animate" onclick="OpenRoomAmenities();">
                    <div class="media block-6 services d-block text-center">
                        <div class="d-flex justify-content-center"><div class="icon"><span class="icon-glass"></span></div></div>
                        <div class="media-body p-2 mt-2">
                            <h3 class="heading mb-3">Room Amenities</h3>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row d-flex">
                <div class="col-md-3 d-flex align-self-stretch ftco-animate" onclick="OpenRooms();">
                    <div class="media block-6 services d-block text-center">
                        <div class="d-flex justify-content-center"><div class="icon"><span class="icon-building-o"></span></div></div>
                        <div class="media-body p-2 mt-2">
                            <h3 class="heading mb-3">Rooms</h3>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </section>
    <div id="myModal" class="modal fade" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <button data-dismiss="modal" class="close locationClose">×</button>
                <div class="modal-header">
                    <h4 class="modal-title" id="headerText"></h4>
                </div>
                <div class="form-group">
                    <div class="col-md-10">
                        <span class="modal-title text-danger" id="footerText"></span>
                    </div>
                </div>
                <div class="modal-body">
                    <div id='myModalContent'> </div>
                </div>
            </div>
        </div>
    </div>

